{
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-await-in-loop": 0
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "jsx": true,
    },
    "sourceType": "module"
  },
  "env": {
    "node": true
  }
}